fun integer(str, lexPos) =
  case Int.fromString(str) of
       NONE => raise Fail("Shouldn't happen: sequence of digits" ^
                          "not recognized as integer--" ^ str)
     | SOME(n) => INT(n, lexPos, lexPos)
  
